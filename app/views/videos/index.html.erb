<div class="box group">
	<h1>
		Videos
		<% if is_admin? %>
			<a class="small-link" href="<%= new_video_url %>">Add Video</a>
		<% end %>
	</h1>

	<ul class="videolist">
		<% @videos.shuffle.each do |video| %>
			<li class="videolist-item">
				<a href="<%= video.url %>">
					<img src="https://img.youtube.com/vi/<%= video.uid %>/mqdefault.jpg"
						alt="<%= video.title %>" title="<%= video.title %>">
	            </a>
				<a href="<%= video.url %>" title="<%= video.title %>">
					<span class="video-title"><%= video.title %></span>
				</a>
				<span class="video-detail"><%= video.author %></span>
				<span class="video-detail"><%= video.duration %></span>
			</li>
		<% end %>
	</ul>
</div>

<script>
	$(function () {
		console.log("asdf");
		$(".videolist a").attr("target", "_blank");
	});
</script>